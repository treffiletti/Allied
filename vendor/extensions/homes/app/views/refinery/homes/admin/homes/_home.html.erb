<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(home) -%>">
  <span class='title'>
    <%= home.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.homes_home_path(home),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_homes_admin_home_path(home),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.homes_admin_home_path(home),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => home.name),
        :method => :delete %>
  </span>
</li>